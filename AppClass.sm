%start Map1::Start
%class AppClass
%header   AppClass.h

%map Map1
%%
// State		Transition		End State		Action(s)
Start
{
 Alth(t_string1: std::string, index: int&) 
	[ ctxt.CheckCreat(t_string1, index) ] Creat_Rel {}
				Alth(t_string1: std::string, index: int&)            See_Rel			{}
				
				
				EOS(t_string: std::string)				OK				{Acceptable(); AddRel(t_string);}
}
Creat_Rel
{
				 Alth(t_string1: std::string, index: int&)			First_Letter_NM  {}
				
}
See_Rel
{
				 Alth(t_string1: std::string, index: int&)			nil				{}
				Num				nil				{}
				Space			Space_See_Rel	{}
				EOS(t_string: std::string)				OK				{Acceptable();}
}
Space_See_Rel
{
				 Alth(t_string1: std::string, index: int&) 
	[ ctxt.CheckJoin(t_string1, index) ]	Join					{}
}
Join
{
			 Alth(t_string1: std::string, index: int&)			nil				{}
			Num				nil				{}
			EOS(t_string: std::string)				OK				{Acceptable();}
}
First_Letter_NM
{
				
				 Alth(t_string1: std::string, index: int&)			nil				{}
				Bracket_right	List_Atr		{}
				
				com				nil				{}
				Num				nil				{}
				
				

}
List_Atr
{
				
				 Alth(t_string1: std::string, index: int&)			First_Letter_AT	{}
				Bracket_left	EndBracket		{}
				com				Comma			{}
				
					
}

First_Letter_AT
{
		
				 Alth(t_string1: std::string, index: int&)			nil				{}
				Bracket_left	EndBracket		{}
				com				Comma			{}
				Num				nil				{}
			
}
Comma
{
			
				 Alth(t_string1: std::string, index: int&)			First_Letter_AT	{}
						
}

space_NM
{
				
				 Alth(t_string1: std::string, index: int&)			First_Letter_NM	{}
				Bracket_right	Error			{}
				Bracket_left	Error			{}
				Space			nil				{}
				
}

EndBracket
{
				
				EOS(t_string: std::string)				OK				{Acceptable(); AddRel(t_string);}
																
}
Default
{
				W_Creat         Error			{}  
				Unknown			Error			{}
				Alth(t_string1: std::string, index: int&)			Error			{}
				Bracket_right	Error			{}
				Bracket_left	Error			{}
				Space			Error			{}
				com				Error			{}
				Num				Error			{}
				Bracket_right_q	Error			{}
				Bracket_left_q	Error			{}
				Creat			Error			{}
				EOS				Error			{}		

}
OK
{
				eps				Start			{}
}

Error
{
				
				EOS				nil				{Unacceptable();}
				eps				Start			{}

}

%%
